{
  "manifest_version": 3,
  "name": "Cadastro Automatico",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://btv2.ksys.net.br/gramnet/bemtevi/documentos/os_imp_tecnico.php*"],
      "js": ["content.js"]
    }
  ]
}
